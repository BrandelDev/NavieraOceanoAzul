@model IEnumerable<NavieraOceanoAzul.Models.Tiquete>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PuertoDestino)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PuertoOrigen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaEmision)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaSalida)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Precio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaLlegada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdbarcoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdclienteNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PuertoDestino)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PuertoOrigen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaEmision)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaSalida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Precio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaLlegada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdbarcoNavigation.Idbarco)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdclienteNavigation.Idcliente)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idtiquete">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idtiquete">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idtiquete">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
